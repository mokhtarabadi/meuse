# Environment variables for Meuse Docker deployment
# Copy this file to .env and customize the values

# PostgreSQL database password
POSTGRES_PASSWORD=meuse_secure_password_change_me

# Meuse frontend session secret (32 characters minimum, use only alphanumeric characters for safety)
# Example: MEUSE_FRONTEND_SECRET=0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdef
MEUSE_FRONTEND_SECRET=your32characteralphanumericsecrethere

# Optional: S3 storage configuration (if using S3 instead of filesystem)
# S3_ACCESS_KEY=your_s3_access_key
# S3_SECRET_KEY=your_s3_secret_key

# Domain for production deployment (used in nginx config and documentation)
DOMAIN=registry.example.com