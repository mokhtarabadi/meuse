# Database configuration
POSTGRES_USER=meuse
POSTGRES_PASSWORD=meuse_secure_password
POSTGRES_DB=meuse
POSTGRES_HOST=postgres

# Meuse configuration
MEUSE_PORT=8855
MEUSE_FRONTEND_SECRET=change_this_to_a_random_string_of_at_least_32_chars

# Admin user
ADMIN_USER=admin
ADMIN_PASSWORD=admin_secure_password
ADMIN_DESCRIPTION="Administrator account"

# Tech user
TECH_USER=tech
TECH_PASSWORD=tech_secure_password
TECH_DESCRIPTION="Technical account for CI/CD"

# Read-only user
READONLY_USER=readonly
READONLY_PASSWORD=readonly_secure_password
READONLY_DESCRIPTION="Read-only account"

# Git server configuration
GIT_SERVER_USER=git
GIT_SERVER_PASSWORD=git_secure_password
GIT_SERVER_PORT=8080

# Git configuration
GIT_EMAIL="registry@meuse.local"
GIT_USERNAME="Meuse Registry"
# Git merge strategy (optional)
# GIT_PULL_REBASE=false
# GIT_MERGE_STYLE=diff3
# GIT_MERGE_FF=only

# Optional: S3 storage configuration
# S3_ACCESS_KEY=your_access_key
# S3_SECRET_KEY=your_secret_key
# S3_ENDPOINT=your_s3_endpoint
# S3_BUCKET=your_bucket_name